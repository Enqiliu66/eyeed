
{
  "version": 2,
  "buildCommand": "",
  "outputDirectory": "docs",
  "public": true,

  "functions": {
    "api/gh-eye.js": {
      "runtime": "@vercel/edge@latest",
      "memory": 128,
      "maxDuration": 10
    }
  },

  "routes": [
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/index.html",
      "dest": "/index.html",
      "headers": {
        "Cache-Control": "public, max-age=60, s-maxage=300"
      }
    },
    {
      "src": "/api/gh-eye",
      "dest": "api/gh-eye.js",
      "headers": {
        "Access-Control-Allow-Origin": "https://Enqiliu66.github.io, http://localhost:5500, http://localhost:3000",
        "Access-Control-Allow-Methods": "POST, OPTIONS",
        "Access-Control-Allow-Headers": "Content-Type",
        "Access-Control-Max-Age": "86400"
      }
    },
    {
      "src": "/(.*\\.(css|js|png|jpg|jpeg|gif|ico|svg))$",
      "dest": "/$1",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    }
  ]
}




