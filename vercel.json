{
  "version": 2,
  "buildCommand": "cp -r docs/* /vercel/output",  
  "outputDirectory": "/vercel/output",
  "public": true,
  "builds": [
    {
      "src": "api/gh-eye.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "index.html"
    },
    {
      "src": "/api/gh-eye",
      "dest": "api/gh-eye.js",
      "methods": ["POST", "OPTIONS"],
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "POST, OPTIONS",
        "Access-Control-Allow-Headers": "Content-Type"
      }
    },
    {
      "src": "/(.*)",
      "dest": "index.html"
    }
  ],
  "regions": ["iad1"]
}
    
